{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "components/client-form-dialog.jsx",
    "app/(pages)/clients/page.jsx",
    "app/(pages)/achats/fournisseurs/page.jsx",
    "components/fournisseur-form-dialog.jsx",
    "components/delete-confirmation-dialog.jsx",
    "components/sidebar.jsx",
    "app/(pages)/ventes/devis/nouveau/page.jsx",
    "app/(pages)/ventes/commandes/nouveau/page.jsx",
    "app/(pages)/ventes/commandes/page.jsx",
    "app/(pages)/produits/page1.jsx",
    "components/product-form-dialog.jsx"
, "app/(pages)/factures/page.jsx", "app/(pages)/ventes/credit-clients/page.jsx", "app/(pages)/ventes/devis/page.jsx", "components/performance-chart.jsx", "components/sppiner.jsx", "app/(pages)/ventes/commandes/test/page.jsx", "app/api/commandes/nouveau/route.js"  ],
  "exclude": ["node_modules"]
}
